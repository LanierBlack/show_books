<%- include header -%>
<form class="form center" method="post">
  <h3>注册</h3>
  <input name="name" type="text" placeholder="用户名(6-16位数字或英文的组合), 不可更改" required />
  <input name="password" type="password" placeholder="密码(6-20位数字或英文的组合)" required />
  <input name="password2" type="password" placeholder="确认密码" required />
  <input name="email" type="email" placeholder="邮箱" required />
  <input type="submit" value="注册" />
  <input type="reset" value="重置" />
</form>
<script>
    <%- include ../public/javascripts/validate.min.js %>
</script>
<%- include footer -%>
